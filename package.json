{
  "name": "used-car",
  "version": "1.0.0",
  "private": true,
  "description": "二手车辆管理系统",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "vercel-build": "pnpm install --no-frozen-lockfile && prisma generate && next build",
    "postinstall": "node -e \"try { require('prisma').postinstall() } catch (e) { }\"",
    "seed": "ts-node prisma/seed.ts",
    "db:optimize": "ts-node --project tsconfig.scripts.json scripts/optimize-db.ts",
    "db:monitor": "ts-node --project tsconfig.scripts.json scripts/db-monitor.ts",
    "db:maintenance": "ts-node --project tsconfig.scripts.json scripts/cron.ts",
    "db:analyze": "ts-node --project tsconfig.scripts.json scripts/db-monitor.ts --analyze-only",
    "db:backup": "ts-node --project tsconfig.scripts.json scripts/db-backup.ts",
    "db:cleanup": "ts-node --project tsconfig.scripts.json scripts/db-cleanup.ts",
    "add-permissions": "ts-node --project tsconfig.scripts.json scripts/add-permissions.ts",
    "add-log-permissions": "ts-node --project tsconfig.scripts.json scripts/add-log-permissions.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.2.0",
    "@prisma/client": "6.3.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-dialog": "1.1.5",
    "@radix-ui/react-dropdown-menu": "2.1.5",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-popover": "1.1.5",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-select": "2.1.5",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-tooltip": "1.1.7",
    "@tailwindcss/forms": "0.5.10",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "10.4.20",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "csstype": "3.1.3",
    "date-fns": "4.1.0",
    "jose": "5.9.6",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.474.0",
    "next": "14.2.23",
    "next-auth": "4",
    "postcss": "8.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.1",
    "swr": "2.3.0",
    "tailwind-merge": "3.0.1",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.8",
    "@types/node": "20.17.16",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.0",
    "prisma": "6.3.0",
    "ts-node": "10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.7.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
